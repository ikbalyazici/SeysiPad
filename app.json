{
  "expo": {
    "name": "SeysiPad",
    "slug": "seysipad",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/5.png",
    "splash": {
      "image": "./assets/5.png",
      "resizeMode": "contain",
      "backgroundColor": "#335cb5"
    },
    "notification": {
      "icon": "./assets/5.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sezeyee.seysipad",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "buildNumber": "2.0.0"
    },
    "android": {
      "package": "com.sezeyee.seysipad",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/5.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": "1.0.0",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/5.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/5.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1aec280b-d1ee-4e71-b352-6f655e7a0410"
      }
    },
    "owner": "sezeyee",
    "updates": {
      "url": "https://u.expo.dev/1aec280b-d1ee-4e71-b352-6f655e7a0410"
    }
  }
}
