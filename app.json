{
  "expo": {
    "name": "SeysiPad",
    "slug": "seysipad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/5.jpeg",
    "splash": {
      "image": "./assets/5.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#335cb5"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sezeyee.seysipad",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.sezeyee.seysipad",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/5.jpeg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/5.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/5.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1aec280b-d1ee-4e71-b352-6f655e7a0410"
      }
    },
    "owner": "sezeyee"
  }
}
